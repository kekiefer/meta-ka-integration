From ebbebe09dab89eabfc610038bf98831b67ec7571 Mon Sep 17 00:00:00 2001
From: Kurt Kiefer <kurt.kiefer@arthrex.com>
Date: Thu, 14 Jul 2022 16:34:17 -0700
Subject: [PATCH] Disable default library stripping

---
 pythran/pythran-linux.cfg  | 2 +-
 pythran/pythran-linux2.cfg | 2 +-
 2 files changed, 2 insertions(+), 2 deletions(-)

diff --git a/pythran/pythran-linux.cfg b/pythran/pythran-linux.cfg
index a5ea209..ddd795b 100644
--- a/pythran/pythran-linux.cfg
+++ b/pythran/pythran-linux.cfg
@@ -5,7 +5,7 @@ include_dirs=
 libs=
 library_dirs=
 cflags=-std=c++11 -fno-math-errno -fvisibility=hidden -fno-wrapv -Wno-unused-function -Wno-int-in-bool-context -Wno-unknown-warning-option
-ldflags=-fvisibility=hidden -Wl,-strip-all
+ldflags=-fvisibility=hidden
 blas=blas
 CC=
 CXX=
diff --git a/pythran/pythran-linux2.cfg b/pythran/pythran-linux2.cfg
index a5ea209..ddd795b 100644
--- a/pythran/pythran-linux2.cfg
+++ b/pythran/pythran-linux2.cfg
@@ -5,7 +5,7 @@ include_dirs=
 libs=
 library_dirs=
 cflags=-std=c++11 -fno-math-errno -fvisibility=hidden -fno-wrapv -Wno-unused-function -Wno-int-in-bool-context -Wno-unknown-warning-option
-ldflags=-fvisibility=hidden -Wl,-strip-all
+ldflags=-fvisibility=hidden
 blas=blas
 CC=
 CXX=
